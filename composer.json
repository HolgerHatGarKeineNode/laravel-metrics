{
    "name": "eliseekn/laravel-metrics",
    "description": "Generate metrics and trends data from your database",
    "keywords": [
        "eliseekn",
        "laravel-metrics",
        "trends"
    ],
    "homepage": "https://github.com/eliseekn/laravel-metrics",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "N'Guessan Kouadio Elis√©e",
            "email": "eliseekn@gmail.com",
            "homepage": "https://eliseekn.netlify.app",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Eliseekn\\LaravelMetrics\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Eliseekn\\LaravelMetrics\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Eliseekn\\LaravelMetrics\\LaravelMetricsServiceProvider"
            ],
            "aliases": {
                "LaravelMetrics": "Eliseekn\\LaravelMetrics\\LaravelMetricsFacade"
            }
        }
    }
}
